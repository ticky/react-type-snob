{
  "name": "react-type-snob",
  "version": "0.0.3",
  "description": "Encourages good typographic details in your React app",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "babel src --out-dir lib --source-maps inline --ignore \"**/*.spec.js\"",
    "test": "jest",
    "lint": "eslint src",
    "lint-and-fixup": "eslint --fix src"
  },
  "keywords": [
    "typography",
    "react",
    "details",
    "ellipses",
    "quotes"
  ],
  "homepage": "https://github.com/ticky/react-type-snob",
  "bugs": {
    "url": "https://github.com/ticky/react-type-snob/issues"
  },
  "repository": "https://github.com/ticky/react-type-snob.git",
  "author": "Jessica Stokes <hello@jessicastokes.net>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-preset-es2015": "6.24.1",
    "babel-runtime": "6.26.0",
    "eslint": "5.8.0",
    "jest": "23.6.0",
    "react": "16.6.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "jest": {
    "collectCoverage": true
  },
  "dependencies": {
    "doc-chomp": "^1.1.0"
  }
}
